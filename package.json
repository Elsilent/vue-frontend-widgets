{
  "name": "@elsilent/vue-frontend-widgets",
  "type": "module",
  "version": "0.1.6",
  "files": [
    "dist"
  ],
  "exports": {
    ".": { "types": "./dist/components.d.ts", "import": "./dist/components.js" },
    "./src/*": "./src/*",
    "./style.css": "./dist/style.css",
    "./styles/fonts/fonts.css": "./dist/styles/fonts/fonts.css",
    "./styles/*.scss": "./dist/styles/*.scss",
    "./utils/clone": { "types": "./dist/utils/clone.d.ts", "import": "./dist/utils/clone.js" },
    "./utils/currency": { "types": "./dist/utils/currency.d.ts", "import": "./dist/utils/currency.js" },
    "./utils/date": { "types": "./dist/utils/date.d.ts", "import": "./dist/utils/date.js" },
    "./utils/error": { "types": "./dist/utils/error.d.ts", "import": "./dist/utils/error.js" },
    "./utils/format": { "types": "./dist/utils/format.d.ts", "import": "./dist/utils/format.js" },
    "./utils/layout_header": { "types": "./dist/utils/layout_header.d.ts", "import": "./dist/utils/layout_header.js" },
    "./utils/line_bar_chart": { "types": "./dist/utils/line_bar_chart.d.ts", "import": "./dist/utils/line_bar_chart.js" },
    "./utils/match": { "types": "./dist/utils/match.d.ts", "import": "./dist/utils/match.js" },
    "./utils/menu": { "types": "./dist/utils/menu.d.ts", "import": "./dist/utils/menu.js" },
    "./utils/mood": { "types": "./dist/utils/mood.d.ts", "import": "./dist/utils/mood.js" },
    "./utils/sort": { "types": "./dist/utils/sort.d.ts", "import": "./dist/utils/sort.js" },
    "./utils/table": { "types": "./dist/utils/table.d.ts", "import": "./dist/utils/table.js" },
    "./utils/theme": { "types": "./dist/utils/theme.d.ts", "import": "./dist/utils/theme.js" }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint src/ --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore",
    "format": "prettier --write src/",
    "up-major": "yarn version --major",
    "up-minor": "yarn version --minor"
  },
  "dependencies": {
    "@fontsource/open-sans": "5.0.28",
    "@fortawesome/fontawesome-free": "6.5.2",
    "@tweenjs/tween.js": "25.0.0",
    "axios": "^1.6.1",
    "element-plus": "2.7.7",
    "flag-icons": "6.7.0",
    "funnel-graph-js": "1.4.2",
    "luxon": "^3.3.0",
    "numeral": "^2.0.6",
    "polymorph-js": "1.0.2",
    "vite-plugin-dts": "^3.6.3",
    "vue": "^3.3.4",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.10.4",
    "@tsconfig/node18": "^2.0.1",
    "@types/jsdom": "^21.1.1",
    "@types/luxon": "^3.3.0",
    "@types/node": "^18.16.17",
    "@types/numeral": "^2.0.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/language-plugin-pug": "^2.1.10",
    "@vue/test-utils": "^2.3.2",
    "@vue/tsconfig": "^0.4.0",
    "eslint": "^8.57.1",
    "eslint-plugin-vue": "^9.31.0",
    "jsdom": "^22.1.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.1",
    "pug": "^3.0.2",
    "sass": "^1.63.4",
    "typescript": "~5.0.4",
    "unplugin-element-plus": "^0.8.0",
    "vite": "^4.3.9",
    "vite-plugin-static-copy": "^2.0.0",
    "vitest": "^0.32.0",
    "vue-tsc": "^1.6.5"
  },
  "resolutions": {
    "@vue/*/braces": "3.0.3"
  },
  "optionalDependencies": {
    "@rollup/rollup-linux-x64-gnu": "*"
  }
}
